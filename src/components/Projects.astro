---
const bubbles = Array.from({ length: 10 }, (_, i) => i + 1);
import myIcon from '../../public/chain.svg';

const arr = [1,1,1,1]

import SingleProject from "../react/single-project.tsx"
---

<div class="projects-title-container">
    <h4 class="projects-title">My Projects</h4>
    <div class="projects-container">
        {arr.map((_, index) => (
            <SingleProject client:load images={["https://pics.freeartbackgrounds.com/midle/Cat_in_the_Forest_Background-1551.jpg", "data:image/jpeg;base64,","https://www.fourpaws.com/-/media/Project/OneWeb/FourPaws/Images/articles/cat-corner/large-cat-breeds/maine-coon-cat-cropped.jpg?h=388&iar=0&w=927&hash=3ECA4FAD349C173A1FBF1752D7253D8A","https://www.fourpaws.com/-/media/Project/OneWeb/FourPaws/Images/articles/cat-corner/large-cat-breeds/bengal-cat.jpg"]} left={index%2 === 0 ? true : false}/>
        ))}
    </div>
    {bubbles.map(i => (
        <div class={`bubble bubble-${i}`}></div>
    ))}
</div>
